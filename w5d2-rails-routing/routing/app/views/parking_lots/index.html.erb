<h1>Parking Lots</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Lat</th>
      <th>Lng</th>
      <th>Size</th>
      <th>Price</th>
      <th>Rating</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parking_lots.each do |parking_lot| %>
      <tr>
        <td><%= parking_lot.name %></td>
        <td><%= parking_lot.address %></td>
        <td><%= parking_lot.lat %></td>
        <td><%= parking_lot.lng %></td>
        <td><%= parking_lot.size %></td>
        <td><%= parking_lot.price %></td>
        <td><%= parking_lot.rating %></td>
        <td><%= link_to 'Show', parking_lot %></td>
        <td><%= link_to 'Edit', edit_parking_lot_path(parking_lot) %></td>
        <td><%= link_to 'Destroy', parking_lot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parking Lot', new_parking_lot_path %>
